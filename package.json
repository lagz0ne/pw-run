{
  "name": "bwsr",
  "version": "0.1.0",
  "description": "Fast CLI for managing browser daemons via Playwright CDP",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "bwsr": "./dist/bwsr"
  },
  "scripts": {
    "build": "bun build --compile --minify src/index.ts --outfile dist/bwsr --external chromium-bidi --external electron",
    "dev": "bun run src/index.ts",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lagz0ne/pw-run"
  },
  "keywords": ["browser", "playwright", "cdp", "automation", "cli", "bun"],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "playwright-core": "^1.57.0",
    "yaml": "^2.3.4"
  }
}
